<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.e_commerceapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ECommerceApp">
        <activity android:name=".pantsPanelActivity"></activity>
        <activity
            android:name=".PurchasingShirts"
            android:label="Details"
            android:parentActivityName=".shirtPanelActivity" />
        <activity android:name=".shirtPanelActivity" />
        <activity android:name=".MainPageStrippedShirt" />
        <activity android:name=".MainPageBlackShirt" />
        <activity android:name=".Confirmation" />
        <activity android:name=".Checkout" />
        <activity android:name=".ShoppingCart" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>